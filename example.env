# Example .env file for llama-swap-sync
# Copy this file and adjust values for your environment

# Model storage path (absolute path recommended for production)
MODELS_PATH=./models

# Config file path
CONFIG_PATH=./config.yaml

# llama-swap web interface port
LLAMA_SWAP_PORT=8080

# GPU configuration
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=compute,utility
GPU_COUNT=1

# Sync script configuration (optional)
# LLAMA_SWAP_CONFIG=./config.yaml
# LLAMA_SWAP_MODELS_DIR=./models
# LLAMA_SWAP_CONTAINER=llama-swap
# LLAMA_SWAP_MAX_BACKUPS=3
# LLAMA_SWAP_MAX_KEY_LENGTH=80
